{
  "compilerOptions": {
    "target": "ES2022",
    "lib": [
      "ES2022"
    ],
    "module": "ES2022",
    "moduleResolution": "Node",
    "outDir": "dist",
    "rootDir": ".",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "noImplicitAny": false
  },
  "include": [
    "src",
    "prisma"
  ],
  "exclude": [
    "node_modules"
  ]
}